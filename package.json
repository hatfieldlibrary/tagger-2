{
  "name": "Tagger",
  "version": "2.2.0",
  "main": "index.js",
  "scripts": {
    "start": "node index",
    "watch": "gulp",
    "dev": "concurrently \"npm start\" \"gulp\"",
    "build-dev": "node index && gulp client.watch",
    "build-dist": "gulp client.build:dist",
    "test-client": "gulp client.unit_test",
    "test-server": "NODE_ENV=test mocha './tests/server/integration/**/*.js' --recursive --check-leaks --reporter spec --compilers js:babel-register",
    "coverage-server": "NODE_ENV=test istanbul cover ./node_modules/mocha/bin/_mocha -- 'tests/server/**/*.js' --compilers js:babel-register",
    "coveralls-server": "NODE_ENV=test istanbul cover ./node_modules/mocha/bin/_mocha -- 'tests/server/**/*.js' --compilers js:babel-register --report lcovonly && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "test-on-travis": "NODE_ENV=test gulp test-on-travis"
  },
  "dependencies": {
    "async": "^2.5.0",
    "body-parser": "^1.17.2",
    "connect-redis": "^3.3.0",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "express": "https://registry.npmjs.org/express/-/express-4.15.4.tgz",
    "express-session": "^1.15.5",
    "helmet": "https://registry.npmjs.org/helmet/-/helmet-3.3.0.tgz",
    "imagemagick": "~0.1.3",
    "lodash": "https://registry.npmjs.org/lodash/-/lodash-4.17.4.tgz",
    "method-override": "^2.3.9",
    "morgan": "^1.8.2",
    "multiparty": "^4.1.2",
    "mysql2": "^1.4.0",
    "passport": "^0.4.0",
    "passport-google-oauth": "^1.0.0",
    "pug": "^2.0.0-rc.3",
    "redis": "^2.6.3",
    "sequelize": "^4.5.0",
    "serve-favicon": "^2.4.3",
    "winston": "^2.3.0"
  },
  "devDependencies": {
    "aliv": "^1.2.0",
    "angular-mocks": "^1.6.1",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-stage-0": "^6.1.18",
    "babel-register": "^6.7.2",
